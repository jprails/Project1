<h1>Listing doctors</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Collegename</th>
    <th>Experience</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @doctors.each do |doctor| %>
  <tr>
    <td><%= link_to doctor.Name,:controller => "appointments", :action => "new", :id => doctor %></td>
    <td><%= doctor.Address %></td>
    <td><%= doctor.City %></td>
    <td><%= doctor.State %></td>
    <td><%= doctor.Zip %></td>
    <td><%= doctor.CollegeName %></td>
    <td><%= doctor.Experience %></td>
    <td><%= link_to 'Show', doctor %></td>
    <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
    <td><%= link_to 'Destroy', doctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
	

<% end %>
</table>




<br />

